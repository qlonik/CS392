\begin_layout Standard
Suppose x 0 , x 1 , .
 .
 .
 , x n are distinct numbers in the interval [a, b] and f ∈ C n+1 [a, b].
 Then, for each x in [a, b], a number ξ(x) (generally unknown) between x
 0 , x 1 , .
 .
 .
 , x n , and hence in (a, b), exists with f (x) = P(x) + f (n+1) (ξ(x))
 (x − x 0 )(x − x 1 ) · · · (x − x n ), (n + 1)! where P(x) is the interpolating
 polynomial given in Eq.
 (3.1).
\end_layout

\begin_layout Standard
From truncated Taylor series expansion:
\begin_inset Formula 
\[
f(x)=f(x_{0})+f'(x_{0})(x-x_{0})+\frac{1}{2}f''(x_{0})(x-x_{0})^{2}+\frac{1}{6}f'''(x_{0})(x-x_{0})^{3}+\frac{1}{24}f^{(4)}(x_{0})(x-x_{0})^{4}+\frac{1}{120}f^{(5)}(\xi)(x-x_{0})^{5}\text{.}
\]

\end_inset


\end_layout

\begin_layout Standard
Therefore, 
\begin_inset Formula 
\[
f(x_{0}-2h)=f(x_{0})-2f'(x_{0})h+2f''(x_{0})h^{2}-\frac{4}{3}f'''(x_{0})h^{3}+\frac{2}{3}f^{(4)}(x_{0})h^{4}-\frac{4}{15}f^{(5)}(\xi)h^{5}\text{,}
\]

\end_inset


\begin_inset Formula 
\[
f(x_{0}-h)=f(x_{0})-f'(x_{0})h+\frac{1}{2}f''(x_{0})h^{2}-\frac{1}{6}f'''(x_{0})h^{3}+\frac{1}{24}f^{(4)}(x_{0})h^{4}-\frac{1}{120}f^{(5)}(\xi)h^{5}\text{,}
\]

\end_inset


\begin_inset Formula 
\[
f(x_{0}+h)=f(x_{0})+f'(x_{0})h+\frac{1}{2}f''(x_{0})h^{2}+\frac{1}{6}f'''(x_{0})h^{3}+\frac{1}{24}f^{(4)}(x_{0})h^{4}+\frac{1}{120}f^{(5)}(\xi)h^{5}\text{,}
\]

\end_inset


\begin_inset Formula 
\[
f(x_{0}+2h)=f(x_{0})+2f'(x_{0})h+2f''(x_{0})h^{2}+\frac{4}{3}f'''(x_{0})h^{3}+\frac{2}{3}f^{(4)}(x_{0})h^{4}+\frac{4}{15}f^{(5)}(\xi)h^{5}\text{.}
\]

\end_inset


\end_layout

\begin_layout Standard
Therefore,
\begin_inset Formula 
\[
f(x_{0}+2h)-f(x_{0}-2h)=4f'(x_{0})h+\frac{8}{3}f'''(x_{0})h^{3}+\frac{8}{15}f^{(5)}(\xi)h^{5}\text{,}
\]

\end_inset


\begin_inset Formula 
\[
f(x_{0}+h)-f(x_{0}-h)=2f'(x_{0})h+\frac{1}{3}f'''(x_{0})h^{3}+\frac{1}{60}f^{(5)}(\xi)h^{5}\text{.}
\]

\end_inset


\end_layout

\begin_layout Standard
Therefore,
\begin_inset Formula 
\[
\frac{8}{3}f'''(x_{0})h^{3}-\frac{2}{3}f'''(x_{0})h^{3}+\frac{8}{15}f^{(5)}(\xi)h^{5}-\frac{1}{30}f^{(5)}(\xi)h^{5}=f(x_{0}+2h)-2f(x_{0}+h)+2f(x_{0}-h)-f(x_{0}-2h)\text{,}
\]

\end_inset


\begin_inset Formula 
\[
2f'''(x_{0})h^{3}-\frac{1}{2}f^{(5)}(\xi)h^{5}=f(x_{0}+2h)-2f(x_{0}+h)+2f(x_{0}-h)-f(x_{0}-2h)\text{,}
\]

\end_inset


\begin_inset Formula 
\[
f'''(x_{0})=\frac{1}{2h^{3}}\left[f(x_{0}+2h)-2f(x_{0}+h)+2f(x_{0}-h)-f(x_{0}-2h)\right]+\frac{h^{2}}{4}f^{(5)}(\xi)\text{.}
\]

\end_inset


\end_layout

