\begin_layout Standard
Since 
\begin_inset Formula $x_{0}<\xi_{0}<x_{0}+2h$
\end_inset

,
\begin_inset Formula 
\[
(x_{0}+2h)^{-1}<\xi_{0}^{-1}<x_{0}^{-1}\text{,}
\]

\end_inset

 
\begin_inset Formula 
\[
\min\left\{ x_{0}^{-2},(x_{0}+2h)^{-2}\right\} <\xi_{0}^{-2}<\max\left\{x_{0}^{-2},(x_{0}+2h)^{-2}\right\}\text{,}
\]

\end_inset


\begin_inset Formula 
\[
\min\left\{ \frac{h^{2}}{3x_{0}^{2}},\frac{h^{2}}{3(x_{0}+2h)^{2}}\right\} <\frac{h^{2}}{3\xi_{0}^{2}}<\max\left\{\frac{h^{2}}{3x_{0}^{2}},\frac{h^{2}}{3(x_{0}+2h)^{2}}\right\}\text{.}
\]

\end_inset


\end_layout

\begin_layout Standard
